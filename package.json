{
    "name": "mvt-api",
    "version": "1.0.0",
    "description": "Virtual Spatial Search Web-API with MapboxVectorTile",
    "keywords": [
        "vector",
        "tile",
        "spatial",
        "search",
        "WebAPI",
        "GeoJSON"
    ],
    "main": "./dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kanahiro/mvt-api.git"
    },
    "author": "Kanahiro Iguchi <kanahiro.iguchi@gmail.com> (https://github.com/Kanahiro)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Kanahiro/mvt-api/issues"
    },
    "homepage": "https://github.com/Kanahiro/mvt-api#readme",
    "dependencies": {
        "@mapbox/tilebelt": "^1.0.2",
        "@mapbox/vt2geojson": "^1.1.5",
        "@turf/boolean-point-in-polygon": "^6.0.1",
        "@turf/boolean-point-on-line": "^6.0.1",
        "@turf/helpers": "^6.1.4"
    },
    "devDependencies": {
        "@types/geojson": "^7946.0.7",
        "@types/node": "^14.14.14",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        },
        "testMatch": [
            "**/tests/**/*.test.ts"
        ],
        "moduleNameMapper": {
            "^#/(.+)": "<rootDir>/src/$1"
        }
    }
}
